<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.ArticlePubStatus" table="articlepubstatus" schema="allcures" >
      
         <id name="status_id" type="java.lang.Integer">
            <column name="status_id" />
            <generator class="auto" />
        </id>
         <property name="status_description" type="string">
            <column name="status_description" length="20"  />
        </property>
           </class>
</hibernate-mapping>
