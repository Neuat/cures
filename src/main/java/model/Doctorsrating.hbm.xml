<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Doctorsrating" table="doctorsrating" schema="allcures" >
        <id name="rate_id" type="java.lang.Integer">
            <column name="rate_id" />
            <generator class="sequence" />
        </id>
        <property name="comments" type="string">
            <column name="comments"  length="40"   />
        </property>
        <property name="ratedBy_id" type="java.lang.Integer">
            <column name="ratedBy_id" length="20"  />
        </property>
         <property name="ratedBy_type_id" type="java.lang.Integer">
            <column name="ratedBy_type_id" length="20"  />
        </property>
        <property name="target_id" type="java.lang.Integer">
            <column name="target_id" length="20"  />
        </property>
        <property name="target_type_id" type="java.lang.Integer">
            <column name="target_type_id" length="20"  />
        </property>
        <property name="ratingVal" type="java.lang.Float">
            <column name="ratingVal" length="20"  />
        </property>
        
    </class>
</hibernate-mapping>
